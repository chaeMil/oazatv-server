{block content}

<table class="table table-condensed table-hover">
    <thead>
        <tr>
            <th>online</th>
            <th>IP</th> 
            <th>OS</th>
            <th>prohlížeč</th>
        </tr>
    </thead>

    <tbody>
        {foreach $aliveUsers as $aliveUser}
            <tr>
                <td>{$aliveUser['alive']}</td>
                <td>{$aliveUser['ip']}</td>
                <td>{$aliveUser['os']}</td>
                <td>{$aliveUser['browser']}</td>
            </tr>
        {/foreach}
    </tbody>
</table>
    
<div class="hidden">
    <p id="viewers-count">{sizeof($aliveUsers)}</p>
</div>
